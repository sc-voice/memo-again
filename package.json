{
  "name": "memo-again",
  "version": "0.6.0",
  "description": "Serializable memoizer with promises",
  "main": "index.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "dependencies": {
    "log-instance": "^1.4.0",
    "merkle-json": "^2.1.0"
  },
  "devDependencies": {
    "eslint-plugin-html": "^4.0.5",
    "mocha": "^10.2.0",
    "should": "^11.2.0"
  },
  "scripts": {
    "build": "scripts/build-docker",
    "test": "npm run test:verbose",
    "test:test": "mocha -w -R min --inline-diffs -g TEST './@(src|test)/**/*.js'",
    "test:verbose": "mocha --inline-diffs",
    "test:watch": "mocha -w --inline-diffs"
  },
  "repository": {
    "tag": "git"
  },
  "keywords": [
    "memoizer",
    "nodejs",
    "file cache",
    "memory cache"
  ],
  "author": "Karl Lew",
  "homepage": "https://github.com/sc-voice/memo-again",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
